  "keywords": [
    "country",
    "code",
    "emoji",
    "flag",
    "ISO",
    "3166-1"
  ],
  "homepage": "https://github.com/fa7ad/emoji-country-code",
  "bugs": {
    "url": "https://github.com/fa7ad/emoji-country-code/issues"
